

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <title>TechnoEdu Öğretmen</title>
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/vendors/owl-carousel-2/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/ÖğrenciPanel/assets/css/style.css">
    <link rel="shortcut icon" href="~/OkulKurs/web/images/TE.png" />
</head>
<body>
    <div class="container-scroller">
        <nav class="sidebar sidebar-offcanvas" id="sidebar">

            <div class="sidebar-brand-wrapper d-none d-lg-flex align-items-center justify-content-center fixed-top">
                <a class="sidebar-brand brand-logo" href="/Ogretmen/Index/"><img src="~/OkulKurs/web/images/TechnoEdu_Logo.png" alt="logo" /></a>
                <a class="sidebar-brand brand-logo-mini" href="index.html"><img src="~/OkulKurs/web/images/TE.png" alt="logo" /></a>
            </div>

            <ul class="nav">
                <li class="nav-item profile">
                    <div class="profile-desc">
                        <div class="profile-pic">
                            <div class="count-indicator">
                                <img class="img-xs rounded-circle " src="~/ÖğrenciPanel/assets/images/faces/face15.jpg" alt="">
                                <span class="count bg-success"></span>
                            </div>
                            <div class="profile-name">
                                <h5 class="mb-0 font-weight-normal">Öğretmen Paneli</h5>
                            </div>
                        </div>
                    </div>
                </li>

                <li class="nav-item menu-items" style="margin-top:20px;">
                    <a class="nav-link" href="/Ogretmen/OgretmenListesi/">
                        <span class="menu-icon">
                            <i class="mdi mdi-format-list-bulleted"></i>
                        </span>
                        <span class="menu-title">Öğretmen Listesi</span>
                    </a>
                </li>


                <li class="nav-item menu-items" style="margin-top:20px;">
                    <a class="nav-link" href="/Ogretmen/OgrenciListesi/">
                        <span class="menu-icon">
                            <i class="mdi mdi-format-list-bulleted"></i>
                        </span>
                        <span class="menu-title">Öğrenci Listesi</span>
                    </a>
                </li>




                <li class="nav-item menu-items" style="margin-top:20px;">
                    <a class="nav-link" href="#">
                        <span class="menu-icon">
                            <i class="mdi mdi-book-open-page-variant"></i>
                        </span>
                        <span class="menu-title">Denemeler</span>
                    </a>
                </li>


                <li class="nav-item menu-items" style="margin-top:20px;">
                    <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false" aria-controls="ui-basic">
                        <span class="menu-icon">
                            <i class="mdi mdi-chart-bar-stacked"></i>
                        </span>
                        <span class="menu-title">İstatislikler</span>
                        <i class="menu-arrow"></i>
                    </a>
                    <div class="collapse" id="ui-basic">
                        <ul class="nav flex-column sub-menu">
                            <li class="nav-item"> <a class="nav-link" href="#">AYT</a></li>
                            <li class="nav-item"> <a class="nav-link" href="#">TYT</a></li>
                        </ul>
                    </div>
                </li>


                <li class="nav-item menu-items" style="margin-top:20px;">
                    <a class="nav-link" href="/Login/OgretmenLogin/">
                        <span class="menu-icon">
                            <i class="mdi mdi-exit-to-app"></i>
                        </span>
                        <span class="menu-title">Çıkış Yap</span>
                    </a>
                </li>


            </ul>
        </nav>

        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_navbar.html -->
            <nav class="navbar p-0 fixed-top d-flex flex-row">
                <div class="navbar-menu-wrapper flex-grow d-flex align-items-stretch">
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-menu"></span>
                    </button>



                    <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                        <span class="mdi mdi-format-line-spacing"></span>
                    </button>

                </div>
            </nav>

            <div class="main-panel">
                @RenderBody()
            </div>
        </div>
    </div>

    <script src="~/ÖğrenciPanel/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/ÖğrenciPanel/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/ÖğrenciPanel/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/ÖğrenciPanel/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/ÖğrenciPanel/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/ÖğrenciPanel/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <script src="~/ÖğrenciPanel/assets/js/off-canvas.js"></script>
    <script src="~/ÖğrenciPanel/assets/js/hoverable-collapse.js"></script>
    <script src="~/ÖğrenciPanel/assets/js/misc.js"></script>
    <script src="~/ÖğrenciPanel/assets/js/settings.js"></script>
    <script src="~/ÖğrenciPanel/assets/js/todolist.js"></script>
    <script src="~/ÖğrenciPanel/assets/js/dashboard.js"></script>
</body>
</html>